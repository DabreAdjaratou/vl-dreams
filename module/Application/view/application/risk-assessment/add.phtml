    function checkPatientRecord(type){
	if(type == 'barcodeid'){
	    checkPatientBarcodeId();
	}else if(type == 'ancid'){
	    checkANCPatientId();
	}else{
	    checkPatientBarcodeId();
	    checkANCPatientId();
	}
    }
    
    function checkPatientBarcodeId(){
	var patientBarcodeId = $('#patientBarcodeId').val();
	var countryId = $('#chosenCountry').val();
        var optCountryId = ($('#country').length) ? $('#country').val():'';
	if($.trim(patientBarcodeId)!= '' && ($.trim(countryId)!= '' || $.trim(optCountryId)!= '')){
            $.post("<?php echo $this->url('check-patient-record'); ?>", {patientBarcodeId: patientBarcodeId,countryId:countryId,optCountryId:optCountryId,type:'clinic',src:'barcodeid'},
            function(data) {
                if ($.trim(data)!= '') { $.blockUI(); document.location.href = data; }
            });
        }
    }
    
    function checkANCPatientId(){
        var ancPatientId = $('#ancPatientId').val();
	var countryId = $('#chosenCountry').val();
        var optCountryId = ($('#country').length) ? $('#country').val():'';
        if($.trim(ancPatientId)!= '' && ($.trim(countryId)!= '' || $.trim(optCountryId)!= '')){
            $.post("<?php echo $this->url('check-patient-record'); ?>", {ancPatientId: ancPatientId,countryId:countryId,optCountryId:optCountryId,type:'clinic',src:'ancid'},
            function(data) {
                if ($.trim(data)!= '') {
		    var conf = confirm("The risk assessment form with ANC Patient ID "+$('#ancPatientId').val()+" has already been submitted. Please click OK if you want to continue editing the same");
		    if(conf){
		       $.blockUI();	
		       document.location.href = data;
		    }else{
		      $('#ancPatientId').val('');
		    }
                }
            });
        }
    }
    
